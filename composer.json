{
    "name": "liues1992/php-protobuf-generator",
    "description": "Google's protobuf generator for PHP",
    "type": "library",
    "keywords": [
        "protocol buffers",
        "protobuf php",
        "grpc"
    ],
    "homepage": "https://github.com/liues1992/php-protobuf-generator",
    "autoload": {
        "psr-4": {
            "Gary\\": "./src/Gary"
        }
    },
    "bin": [
        "protoc-gen.php",
        "protoc-gen-plugin.php",
        "protoc-gen-grpc-plugin.php"
    ],
    "authors": [
        {
            "name": "Gary Lyn",
            "email": "liues1992@gmail.com",
            "homepage": "https://github.com/liues1992",
            "role": "Original Author"
        }
    ],
    "support": {
        "issues": "https://github.com/liues1992/php-protobuf-generator/issues"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "require": {
        "php": ">=7.0",
        "pear/console_commandline": "^1.2",
        "lvht/protobuf": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "license": "MIT"
}
